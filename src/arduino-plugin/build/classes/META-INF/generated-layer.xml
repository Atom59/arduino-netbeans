<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Templates">
        <folder name="Project">
            <folder name="Arduino">
                <file name="ArduinoProject.zip" url="nbresloc:/arduino/ArduinoProject.zip">
                    <!--arduino.ArduinoWizardIterator-->
                    <attr boolvalue="true" name="template"/>
                    <attr
                        bundlevalue="arduino.Bundle#Arduino_displayName" name="displayName"/>
                    <attr name="iconBase" stringvalue="arduino/Arduino.png"/>
                    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/arduino/ArduinoDescription.html"/>
                    <attr name="instantiatingIterator" newvalue="arduino.ArduinoWizardIterator"/>
                    <attr boolvalue="true" name="requireProject"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
